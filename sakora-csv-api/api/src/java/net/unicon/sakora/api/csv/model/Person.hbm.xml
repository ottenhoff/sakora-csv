<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="net.unicon.sakora.api.csv.model.Person" table="SAKORA_PERSON">
      <id name="userEid" type="string" length="255" column="USER_EID" />
      <property name="userId" type="string" length="255" column="USER_ID" />
      <property name="inputTime" type="integer" column="INPUT_TIME" />
   </class>
   
</hibernate-mapping>

